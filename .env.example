# Mapbox API Access Token (get from https://account.mapbox.com/access-tokens/)
MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# Database (for local development, docker-compose sets these)
DATABASE_URL=jdbc:postgresql://localhost:5432/str_platform
DATABASE_USER=str_user
DATABASE_PASSWORD=str_password

# Redis (for local development)
REDIS_HOST=localhost
REDIS_PORT=6379

# RabbitMQ (for local development)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=str_user
RABBITMQ_PASSWORD=str_password

# Spring Profile (dev, prod, test)
SPRING_PROFILES_ACTIVE=dev
